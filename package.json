{
  "name": "open-inventor",
  "displayName": "Open Inventor",
  "description": "Syntax highlighting and editing support for Open Inventor scene graphs",
  "icon": "images/hello_cone.png",
  "version": "1.0.0",
  "publisher": "Renreok",
  "repository": {
    "type": "git",
    "url": "https://github.com/Renreok/vsce-open-inventor"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "iv",
      "aliases": ["Open Inventor Scene Graph", "iv"],
      "extensions": [".iv"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "iv",
      "scopeName": "source.iv",
      "path": "./syntaxes/iv.tmLanguage.json"
    }]
  }
}
